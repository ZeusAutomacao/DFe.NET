											<xs:documentation>Dados do endereço</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="email" type="TEmail" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Endereço de email</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
